<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="root" style="-fx-background-color: white;" stylesheets="@sample.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vztekoverflow.lospiratos.sample.SampleController">
   <children>
      <Slider fx:id="XSlider" max="100000" min="-100000.0" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets right="30.0" />
         </StackPane.margin></Slider>
       <Slider fx:id="YSlider" max="100000" min="-100000.0" orientation="VERTICAL" StackPane.alignment="CENTER_RIGHT">
         <StackPane.margin>
            <Insets right="15.0" />
         </StackPane.margin></Slider>
       <Slider fx:id="ScaleSlider" max="4.0" min="0.2" orientation="VERTICAL" value="1.0" StackPane.alignment="CENTER_RIGHT">
         <StackPane.margin>
            <Insets />
         </StackPane.margin></Slider>
   </children>
</StackPane>
